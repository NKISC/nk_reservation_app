{
	// 如果您是通过uni_modules形式引入uView，可以忽略此配置
	"easycom": {
		"autoscan": false,
		"custom": {
			"^u-(.*)": "@/uni_modules/uview-ui/components/u-$1/u-$1.vue",
			"^my-(.*)": "@/components/my-$1/my-$1.vue"
		}

	},
	"pages": [
		{
			"path": "pages/subscribe/subscribe",
			"style": {
				"navigationBarTitleText": "预约",
				"navigationStyle": "custom"
			}

		}, {
			"path": "pages/Individual/Individual",
			"style": {
				"navigationBarTitleText": "个人",
				"navigationStyle": "custom"
			}
		}
	],
	"subPackages": [{
		"root": "pages_individual",
		"pages": [
			{
				"path": "Individual/ReservationDetails",
				"style": {
					"navigationBarTitleText": "",
					"navigationStyle": "custom"
				}
			}, {
				"path": "Individual/checkReservation",
				"style": {
					"navigationBarTitleText": "",
					"navigationStyle": "custom"
				}
			}, {
				"path": "Individual/alterReservationDetails",
				"style": {
					"navigationBarTitleText": "",
					"navigationStyle": "custom"
				}
			}
		]
	}],
	"tabBar": {
		"color": "#9E9E9E ",
		"selectedColor": "#754E73",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"borderStyle": "black",
		"position": "bottom",
		"fontSize": "14px",
		"iconWidth": "20px",
		"height":"70px",
		"list": [{
			"pagePath": "pages/subscribe/subscribe",
			"text": "预约",
			"iconPath": "./static/yy_unactive.jpeg",
			"selectedIconPath": "./static/yy_active.jpeg"
		}, {
			"pagePath": "pages/Individual/Individual",
			"text": "个人",
			"iconPath": "./static/gr_unactive.jpeg",
			"selectedIconPath": "./static/gr_active.jpeg"
		}]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}